{
  "name": "mangaclone-local-api",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "ts-node src/seed.ts"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.2",
    "minio": "8.5.3",
    "prisma": "5.11.0",
    "@prisma/client": "5.11.0"
  },
  "devDependencies": {
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2"
  }
}